  "description": "Common reactjs components for apps",
  "private": false,
  "main": "lib/index.js",
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "NODE_ENV=production babel src --out-dir lib && cp package.json README.md LICENSE.md lib/",
